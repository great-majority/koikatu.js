{
  "name": "koikatu.js",
  "version": "0.1.0",
  "description": "Browser-compatible parser for Koikatu/Honeycome character cards",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check ./src ./tests",
    "lint:fix": "biome check --write ./src ./tests"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/great-majority/koikatu.js.git"
  },
  "license": "MIT"
}
